@model IEnumerable<MojecStockERP.Models.MeterOrder>
@{
    ViewBag.Title = "MeterProcurement";
    Layout = "~/Views/Shared/Procurement.cshtml";
}


<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Meter Procurement</h1>


    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">Meter Procurement</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                    <thead class="text-capitalize">
                        <tr>
                            
                            <th>
                                @Html.DisplayNameFor(model => model.MeterType)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Partners)
                            </th>

                            <th>
                                @Html.DisplayNameFor(model => model.Quantity)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.BillofLaden)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Date)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Arrivaldate)
                            </th>
                        </tr>
                    </thead>
                    @foreach (var item in Model)
                    {
                <tr>

                    <td>
                        @Html.DisplayFor(modelItem => item.MeterType)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Partners)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Quantity)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.BillofLaden)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Date)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Arrivaldate)
                    </td>
                </tr>
                    }

                </table>
            </div>
        </div>
    </div>

</div>


